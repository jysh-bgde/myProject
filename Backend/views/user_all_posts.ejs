<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title><%= title %></title>
</head>
<body>
  <%= title %>

  <ul>
    <% list_user_posts.forEach(function(post){ %>
      <li><a href= "<%= post.url %>" > <%= post.name %> - </a><%= post.user_id %> -<%= post.details%></li>
      <% }) %>
  </ul>
</body>
</html>